generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"

}


model User{
  id String @id @default(cuid())
  email String @unique
  password String
  verified Boolean @default(false)
  balance BigInt @default(50000000)
}

model Trade{
  id String @id @default(cuid())
  market String
}